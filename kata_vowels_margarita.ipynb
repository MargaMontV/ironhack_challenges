   "source": [
    "def get_count(sentence):\n",
    "    vowel = \"aeiou\"\n",
    "    count = 0\n",
    "    for letter in sentence:\n",
    "        if letter in vowel:\n",
    "            count += 1\n",
    "    return count\n"
   ]
